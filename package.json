{
    "name": "doctypes",
    "displayName": "DocTypes",
    "description": "JSDoc generator for TypeScript using AI.",
    "version": "1.0.0",
    "publisher": "dalirnet",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "typescript",
        "documentation",
        "jsdoc"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onCommand:doctypes.generate"
    ],
    "main": "dist/extension.js",
    "contributes": {
        "commands": [
            {
                "when": "editorLangId == typescript",
                "command": "doctypes.generate",
                "title": "DocTypes generate"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorLangId == typescript",
                    "command": "doctypes.generate",
                    "group": "1_modification"
                }
            ]
        },
        "keybindings": [
            {
                "when": "editorLangId == typescript",
                "command": "doctypes.generate",
                "key": "ctrl+alt+d",
                "mac": "cmd+alt+d"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "test": "node ./dist/test/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dalirnet/doctypes.git"
    },
    "author": "Amir Reza Dalir <dalirnet@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/lodash": "^4.14.181",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.66.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "axios": "^0.26.1"
    }
}
